{
  "name": "restweb-h",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn --clear src/app.ts",
    "dev:http": "tsnd --respawn --clear src/app.http.ts",
    "dev:http2": "tsnd --respawn --clear src/app.http2.ts",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/app.js",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:migrate:test": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test": "npm run prisma:migrate:test && jest",
    "test:watch": "npm run prisma:migrate:test && jest --watch",
    "test:coverage": "npm run prisma:migrate:test && jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "jest": "^30.2.0",
    "prisma": "^5.22.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.4.5",
    "env-var": "^7.5.0",
    "express": "^4.21.1"
  }
}
